}

.cancel-btn:hover {
  background: linear-gradient(135deg, #d1d5db 0%, #b7b9c0 100%);
  border-color: #9768E1;
  box-shadow: 0 6px 18px rgba(151, 104, 225, 0.2);
  transform: translateY(-2px);
}

.cancel-btn::before {
  content: '';
  position: absolute;
  top: -2px;
  left: -2px;
  right: -2px;
  bottom: -2px;
  border: 2px solid #9768E1;
  border-radius: 10px;
  opacity: 0;
  transition: opacity 0.3s ease-in-out;
  pointer-events: none;
}

.cancel-btn:hover::before {
  opacity: 1;
  animation: borderAnimation 0.5s ease-in-out;
}

@keyframes borderAnimation {
  0% {
    transform: scale(1);
    opacity: 0;
  }

  50% {
    transform: scale(1.02);
    opacity: 1;
  }

  100% {
    transform: scale(1);
    opacity: 1;
  }
}

.view-details .detail-row {
  display: flex;
  padding: 15px 0;
  border-bottom: 1px solid #eee;
}

.view-details .detail-row:last-child {
  border-bottom: none;
}

.label {
  width: 150px;
  font-weight: 500;
  color: #333;
}

.value {
  flex-grow: 1;
  color: #666;
}

.password-form {
  max-width: 500px;
}

/* Notifications Page */
.notifications-list {
  display: flex;
  flex-direction: column;
  gap: 15px;
}

.notification-item {
  padding: 20px;
  border-radius: 10px;
  transition: all 0.3s ease;
  border: 1px solid #ccc;
  border-left: 4px solid transparent;
}

.notification-item:hover {
  background-color: transparent;
}

.notification-item.unread {
  border: 1px solid #9768E1;
  border-left: 4px solid #9768E1;
}

.notification-content h4 {
  margin: 0 0 10px 0;
  color: #333;
}

.notification-content p {
  margin: 5px 0;
  color: #666;
}

.notification-meta {
  display: flex;
  gap: 15px;
  margin-top: 10px;
  font-size: 0.9rem;
}

.time {
  color: #999;
}

.author {
  color: #9768E1;
  font-weight: 500;
}

.unread-indicator {
  color: #9768E1;
  font-weight: 500;
}

.notification-actions {
  display: flex;
  gap: 10px;
  margin-top: 15px;
}

.mark-read-btn,
.delete-btn {
  padding: 5px 10px;
  border: none;
  border-radius: 3px;
  cursor: pointer;
  font-size: 0.9rem;
  transition: all 0.3s ease;
}

.mark-read-btn {
  background-color: #9768E1;
  color: white;
}

.mark-read-btn:hover {
  background-color: #7d52c9;
}

.delete-btn {
  background-color: #f44336;
  color: white;
}

.delete-btn:hover {
  background-color: #d32f2f;
}

.history-list {
  display: flex;
  flex-direction: column;
  gap: 15px;
}

.history-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 15px;
  border-radius: 5px;
}

.test-info h4 {
  margin: 0 0 5px 0;
  color: #333;
}

.test-info p {
  margin: 0;
  color: #666;
}

.test-status {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  gap: 5px;
}

.test-status .score {
  font-weight: bold;
  color: #9768E1;
  display: none;
}

.test-status .upcoming {
  color: #FFC107;
  font-weight: 500;
}

/* Pagination */
.student-pagination {
  display: flex;
  justify-content: center;
  align-items: center;
  gap: 8px;
  margin-top: 20px;
}

.pagination-pages {
  display: flex;
  align-items: center;
  gap: 4px;
}

.pagination-button {
  min-width: 34px;
  height: 34px;
  padding: 0 10px;
  border-radius: 999px;
  border: 1px solid #d1d5db;
  background-color: transparent;
  color: #374151;
  font-size: 0.85rem;
  cursor: pointer;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  transition: background-color 0.15s ease, border-color 0.15s ease, color 0.15s ease, box-shadow 0.15s ease;
}

.pagination-button:hover:not(:disabled) {
  background-color: #f3f4f6;
}

.pagination-button:disabled {
  opacity: 0.5;
  cursor: not-allowed;
}

.pagination-button:focus-visible {
  outline: 2px solid #523C48;
  outline-offset: 2px;
}

.pagination-page.active {
  background-color: #9768E1;
  border-color: #9768E1;
  color: #ffffff;
  box-shadow: 0 0 0 1px rgba(151, 104, 225, 0.6);
}

.pagination-ellipsis {
  padding: 0 6px;
  color: #9ca3af;
  font-size: 0.85rem;
  user-select: none;
}

.pagination-prev-next {
  padding-inline: 14px;
}

/* Answers table on Result Detail */
.answers-table-wrapper {
  width: 100%;
  overflow-x: auto;
}

.answers-table {
  width: 100%;
  border-collapse: collapse;
  min-width: 600px;
}

.answers-table thead {
  background-color: #9768E1;
  color: #ffffff;
}

.answers-table th,
.answers-table td {
  padding: 10px 14px;
  text-align: left;
  font-size: 0.9rem;
}

.answers-table tbody tr:nth-child(even) {
  background-color: #f9fafb;
}

.answers-table tbody tr:hover {
  background-color: #f3f4f6;
}

.answer-question-cell .answer-question-label {
  font-weight: 600;
  color: #111827;
}

.answer-question-cell .answer-question-text {
  margin-top: 4px;
  font-size: 0.85rem;
  color: #4b5563;
}

.answer-text {
  font-size: 0.9rem;
  color: #111827;
}

.answer-text-muted {
  color: #9ca3af;
}

.status-pill {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 4px 12px;
  border-radius: 999px;
  font-size: 0.8rem;
  font-weight: 600;
  color: #ffffff;
}

.status-correct {
  background-color: #4caf50;
}

.status-incorrect {
  background-color: #e74c3c;
}

.status-unattempted {
  background-color: #9ca3af;
}

/* Mobile Styles */
@media (max-width: 768px) {
  .std_hamburger-menu {
    display: flex;
  }

  .dashboard-sidebar {
    position: fixed;
    top: 0;
    left: 0;
    height: 100%;
    transform: translateX(-100%);
  }

  .dashboard-sidebar.open {
    transform: translateX(0);
  }

  .sidebar-overlay {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 99;
  }

  .student-dashboard {
    flex-direction: column;
  }

  .dashboard-sidebar {
    width: 250px;
    height: 100%;
    max-width: 80%;
    right: -250px;
    transition: transform 0.3s ease;
  }

  .dashboard-sidebar.open {
    transform: translateX(-100%);
  }

  .sidebar-menu {
    display: block;
    padding: 0;
    margin: 0;
  }

  .sidebar-menu li {
    flex: none;
    border-bottom: 1px solid rgba(255, 255, 255, 0.05);
    border-right: none;
    margin: 0;
    padding: 0;
  }

  .std_stats-grid {
    grid-template-columns: 1fr 1fr;
  }

  .assessments-grid {
    grid-template-columns: 1fr;
  }

  .table-header,
  .table-row {
    grid-template-columns: 1fr 1fr;
  }

  .table-header div:nth-child(3),
  .table-header div:nth-child(4),
  .table-row div:nth-child(3),
  .table-row div:nth-child(4) {
    display: none;
  }

  .history-item {
    flex-direction: column;
    align-items: flex-start;
    gap: 10px;
  }

  .test-status {
    align-items: flex-start;
  }

  .profile-dropdown {
    width: 280px;
    right: -20px;
  }

  .dashboard-content {
    padding-left: 30px;
    /* Reset padding on mobile */
  }

  .dashboard-home {
    padding-left: 0;
    /* Reset padding on mobile */
  }
}